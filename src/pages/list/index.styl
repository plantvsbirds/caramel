.head
  height 10em

.col
  width 100%
  display flex
  flex-direction row
  justify-content space-evenly
  position relative
  margin-top 1em

  .item
    flex 0 0 1
    .title,
    .content
      cursor pointer
    .title
      margin 1em auto 0em auto
      font-weight 600
      &:hover
        color rgb(0, 122, 255)
    .content
      margin-top .5em